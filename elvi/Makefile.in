install-elviSCRIPTS: $(elvi_SCRIPTS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(elvidir)
	@list='$(elvi_SCRIPTS)'; for p in $$list; do \
	  if test -f $$p; then \
	    echo " $(INSTALL_SCRIPT) $$p $(DESTDIR)$(elvidir)/`echo $$p|sed '$(transform)'`"; \
	    $(INSTALL_SCRIPT) $$p $(DESTDIR)$(elvidir)/`echo $$p|sed '$(transform)'`; \
	  else if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_SCRIPT) $(srcdir)/$$p $(DESTDIR)$(elvidir)/`echo $$p|sed '$(transform)'`"; \
	    $(INSTALL_SCRIPT) $(srcdir)/$$p $(DESTDIR)$(elvidir)/`echo $$p|sed '$(transform)'`; \
	  else :; fi; fi; \
	done

uninstall-elviSCRIPTS:
	@$(NORMAL_UNINSTALL)
	list='$(elvi_SCRIPTS)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(elvidir)/`echo $$p|sed '$(transform)'`; \
	done

